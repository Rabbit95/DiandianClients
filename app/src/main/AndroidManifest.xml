<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.diandianclient">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_rounds"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_rounds"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
            </intent-filter>
        </activity>

        <activity android:name=".Activity.MainActivity"></activity>
        <activity android:name=".Activity.LoginActivity"></activity>
        <activity android:name=".Activity.CartActivity"></activity>
        <activity android:name=".Activity.RegisteredActivity" ></activity>
        <activity android:name=".Activity.MyActivity" ></activity>
        <activity android:name=".Activity.ForgetActivity"></activity>
        <activity android:name=".Activity.MyInfoActivity"></activity>
        <activity android:name=".Activity.OrderActivity"></activity>
        <activity android:name=".Activity.OldOrderListActivity"></activity>
        <activity android:name=".Activity.OldOrderActivity"></activity>
        <activity android:name=".Activity.WEBViewActivity"></activity>
        <activity android:name=".Activity.PaySuccessActivity"></activity>
    </application>


</manifest>